{
  "name": "demo-credit",
  "version": "1.0.0",
  "description": "A mobile lending app that requires wallet functionality.",
  "main": "dist/server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:migrate": "cross-env NODE_ENV=test knex migrate:latest",
    "test:rollback": "cross-env NODE_ENV=test knex migrate:rollback",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "knex:migrate:latest:test": "npx knex --knexfile src/database/knexfile.ts migrate:latest --env test",
    "knex:migrate:rollback:test": "npx knex --knexfile src/database/knexfile.ts migrate:rollback --env test",
    "knex:seed:run:test": "npx knex --knexfile src/database/knexfile.ts seed:run --env test",
    "knex": "npx knex --knexfile src/database/knexfile.ts",
    "knex1": "npx knex --knexfile dist/database/knexfile.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.6",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.14.9",
    "@types/nodemailer": "^6.4.15",
    "@types/nodemailer-smtp-transport": "^2.7.8",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  }
}
